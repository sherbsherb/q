  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "nodemon server.js",
    "all": "concurrently \"npm run server\" \"npm run dev\"",
    "lint": "npx eslint . --ext .js,.ts,.tsx,.jsx",
    "fix": "npx eslint . --ext .js,.ts,.tsx,.jsx --fix",
